@Styles.Render("~/Style/datatable")
<link href="@Url.Content("~/Extension/toastr-master/toastr.min.css")" rel="stylesheet" />
<script src="@Url.Content("~/Extension/toastr-master/toastr.min.js")"></script>
<div class="row">
    <div class="clearfix"></div>
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h1 style="display:inline-block">@ViewBag.title</h1>
                <ul class="nav navbar-right panel_toolbox">
                    <li>
                        <button class="btn btn-success" onclick="window.location.href='@Url.Content("~/User/Create")'"><i class="fa fa-user-plus"></i> Thêm tài khoản</button>
                    </li>
                </ul>
                <div class="clearfix"></div>
            </div>

            <div class="x_content">

                <table id="datatable-user" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th>Tên đăng nhập</th>
                            <th>Họ và tên</th>
                            <th>Email</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/Scripts/datatable")
<script src="@Url.Content("~/Scripts/user.js")"></script>